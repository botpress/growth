{
  "name": "@bp-templates/mailerlite",
  "integrationName": "mailerlite",
  "scripts": {
    "build": "bp add -y && bp build",
    "check:type": "tsc --noEmit",
    "check:bplint": "bp lint",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "check:lint": "pnpm check:eslint && pnpm check:prettier",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier --write .",
    "fix:lint": "pnpm fix:eslint && pnpm fix:prettier"
  },
  "private": true,
  "dependencies": {
    "@botpress/client": "1.24.1",
    "@botpress/sdk": "4.15.6",
    "@mailerlite/mailerlite-nodejs": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.16.4",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3"
  }
}
